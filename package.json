  },
  "keywords": [
    "Flow",
    "Rage",
    "GTAV",
    "Multiplayer",
    "Development",
    "Gamemode"
  ],
  "author": {
    "email": "vernetti@kenift.com",
    "name": "Paulo Vernetti",
    "url": "https://github.com/PauloVernetti"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FlowRage/core/issues"
  },
  "homepage": "https://github.com/FlowRage/core#readme",
